<li id="<%="party_song_#{party_song_id}"%>" class="flex items-center justify-between h-14 hover:bg-spotify-gray-clear">
    <div id="<%= song.spotify_song_id %>" class="flex items-center gap-2 flex-grow min-w-0">
        <img src="<%= song.image %>" class="w-14 h-14">
        <div class="overflow-hidden text-ellipsis whitespace-nowrap">
          <p class="font-semibold text-md truncate"><%= song.name %></p>
            <p class="font-semibold text-gray-500 text-sm truncate"><%= song.artists.join(',') %></p>
        </div>
    </div>
    <div class="border-2 border-gray-200 rounded-xl px-4 py-1 font-semibold mr-4 hover:bg-gray-200 hover:text-black">
        Vote
    </div>
</li>
